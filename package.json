{
  "name": "egg-ssr",
  "version": "1.0.0",
  "description": "Egg + React + React-Router,  SPA + SSR",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.dev.js",
    "dev:server": "cross-env NODE_ENV=development webpack --watch --config ./build/webpack.config.server.js",
    "dev": "npm run dev:client",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.server.js",
    "build": "npm run build:client && npm run build:server",
    "start": "cross-env DEBUG=app:*,edge:* nodemon server.js",
    "analyz": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.analyz.js",
    "lint": "eslint --ext .jsx,.js .",
    "lint:error": "npm run lint -- --quiet",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint:fix": "npm run stylelint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yanchenyu/egg-ssr.git"
  },
  "author": "yancy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yanchenyu/egg-ssr/issues"
  },
  "config": {
    "vd": "/m/",
    "port": 9999,
    "env": "dev"
  },
  "homepage": "https://github.com/Yanchenyu/egg-ssr#readme",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@babel/runtime-corejs2": "^7.8.4",
    "egg": "^2.11.2",
    "egg-view-nunjucks": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.1.0",
    "husky": "^4.2.3",
    "ignore-loader": "^0.1.2",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "pm2": "^4.2.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
